{
  "name": "excelante-front",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "stylelint \"styles/**/*.scss\" && eslint --ext .js --ext .jsx \"pages/**/*\"",
    "lint:fix": "stylelint \"styles/**/*.scss\" --fix && eslint --ext .js --ext .jsx --fix \"pages/**/*\""
  },
  "dependencies": {
    "@ant-design/dark-theme": "^2.0.1",
    "@ant-design/icons": "^4.1.0",
    "@zeit/next-less": "^1.0.1",
    "ace-builds": "^1.4.11",
    "antd": "^4.2.4",
    "next": "9.3.6",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-ace": "^9.1.1",
    "react-app-rewire-antd-theme": "^1.1.4",
    "react-dom": "16.13.1",
    "styled-components": "^5.1.0",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@zeit/next-sass": "^1.0.1",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "husky": "^4.2.5",
    "less": "^3.11.1",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.2.3",
    "next-compose-plugins": "^2.2.0",
    "null-loader": "^4.0.0",
    "prettier": "^2.0.5",
    "sass": "^1.26.5",
    "stylelint": "^13.4.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.17.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "styles/**/*.scss": [
      "stylelint --fix",
      "prettier --config .prettierrc --write"
    ],
    "pages/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --config .prettierrc --write"
    ]
  }
}
